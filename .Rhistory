oneway <- function(formula, data, ...) {
#anova
fit <- lm(formula, data)
# summary statistics
stats <- aggregate(formula, data,
function(x) c(n=length(x),
mean = mean(x),
sd=sd(x)))
result <- list(anova=fit, summarystats=stats)
return(result)
}
